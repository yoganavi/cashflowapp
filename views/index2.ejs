<form method="post" action="/cobaya" class="seca pilihbulan">
  <input name="bulan" type="month" value=<%= today %>  >
  <!-- <button>submit</button> -->
</form>

<form class="col s12">
  <div class="input-field col s12">
    <select>
      <option value="" disabled selected>payment method</option>
      <option value="1">cimb</option>
      <option value="2">mega</option>
      <option value="3">gopay</option>
      <option value="3">gopaylatter</option>
      <option value="3">debit</option>
    </select>
  </div>
  <div class="row">
    <div class="input-field col s6">
      <input id="last_name" type="text" class="validate">
      <label for="last_name">Harga</label>
    </div>
  </div>
  <div class="row">
    <div class="input-field col s6">
      <input id="deskripsi" type="text" class="validate">
      <label for="deskripsi">Deskripsi</label>
    </div>
  </div>
</form>
  
<div class="fixed-action-btn">
  <a class="btn-floating btn-large red">
    <i class="large material-icons">mode_edit</i>
  </a>
  <ul>
    <li><a class="btn-floating red">C</a></li>
    <li><a class="btn-floating yellow darken-1"><i class="material-icons">format_quote</i></a></li>
    <li><a class="btn-floating green"><i class="material-icons">publish</i></a></li>
    <li><a class="btn-floating blue"><i class="material-icons">attach_file</i></a></li>
  </ul>
</div>